#, no-wrap
msgid "[[!inline pages=\"donate/testimonials.inline\" raw=\"yes\" sort=\"age\"]]\n"
msgstr "[[!inline pages=\"donate/testimonials.inline.ca\" raw=\"yes\" sort=\"age\"]]\n"

