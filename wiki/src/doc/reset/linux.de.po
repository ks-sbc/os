msgid "[[!meta title=\"Resetting a USB stick using Linux\"]]\n"
msgstr "[[!meta title=\"Einen USB-Stick unter Linux zurücksetzen\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!inline pages=\"doc/reset.intro\" raw=\"yes\" sort=\"age\"]]\n"
msgstr "[[!inline pages=\"doc/reset.intro.de\" raw=\"yes\" sort=\"age\"]]\n"

