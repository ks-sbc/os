msgid "<h1 id=\"security\">The <i>Unsafe Browser</i> is not anonymous</h1>\n"
msgstr "<h1 id=\"security\">Il <i>Browser Non Sicuro</i> non è anonimo</h1>\n"

#. type: Plain text
#, no-wrap
