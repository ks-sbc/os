#. type: Plain text
#, no-wrap
msgid "[[!tag security/fixed]]\n"
msgstr "[[!tag security/fixed]]\n"

#. type: Bullet: '      1. '
msgid "To change your passphrase, execute the following command."
msgstr "Per canviar la contrasenya, executeu l'ordre següent."

#. type: Plain text
#, no-wrap
msgid ""
"         Replace <span class=\"command-placeholder\">[partition]</span> with the\n"
"         partition name found in step 1.6.\n"
msgstr ""
"         Substituïu <span class=\"command-placeholder\">[partició]</span> amb el\n"
"         nom de la partició trobat al pas 1.6.\n"

#. type: Plain text
#, no-wrap
msgid "        <p class=\"pre command-template\">cryptsetup luksChangeKey /dev/<span class=\"command-placeholder\">[partition]</span></p>\n"
msgstr ""
"        <p class=\"pre command-template\">cryptsetup luksChangeKey /dev/"
"<span class=\"command-placeholder\">[partició]</span></p>\n"
