msgid "[[!meta title=\"Tails 5.1.1 is out\"]]\n"
msgstr "[[!meta title=\"Tails 5.1.1 is out\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Thu, 23 Jun 2022 10:00:00 +0000\"]]\n"
msgstr "[[!meta date=\"Thu, 23 Jun 2022 10:00:00 +0000\"]]\n"

#. type: Plain text
