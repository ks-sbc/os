msgid "[[!meta title=\"Tails 5.0 is out\"]]\n"
msgstr "[[!meta title=\"Ha salido Tails 5.0\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Tue, 03 May 2022 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Tue, 03 May 2022 12:34:56 +0000\"]]\n"

#. type: Plain text
