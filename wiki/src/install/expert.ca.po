#, no-wrap
msgid "[[!inline pages=\"install/inc/steps/welcome.inline\" raw=\"yes\" sort=\"age\"]]\n"
msgstr "[[!inline pages=\"install/inc/steps/welcome.inline.ca\" raw=\"yes\" sort=\"age\"]]\n"

